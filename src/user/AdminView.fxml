<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="user.AdminViewController">
   <children>
      <TableView fx:id="userTable" editable="true" layoutX="15.0" layoutY="14.0" prefHeight="318.0" prefWidth="671.0">
        <columns>
          <TableColumn fx:id="userIDColumn" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="firstNameColumn" prefWidth="75.0" text="First" />
            <TableColumn fx:id="lastNameColumn" prefWidth="75.0" text="Last" />
            <TableColumn fx:id="userNameColumn" prefWidth="75.0" text="User" />
            <TableColumn fx:id="passwordColumn" prefWidth="75.0" text="Password" />
            <TableColumn fx:id="saltColumn" prefWidth="75.0" text="Blob" />
            <TableColumn fx:id="sinColumn" prefWidth="75.0" text="SIN" />
            <TableColumn fx:id="phoneColumn" prefWidth="75.0" text="Phone" />
            <TableColumn fx:id="emailColumn" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="dobColumn" prefWidth="75.0" text="D.O.B" />
            <TableColumn fx:id="isAdminColumn" prefWidth="75.0" text="Admin" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="editUserButton" layoutX="14.0" layoutY="359.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="61.0" text="Edit" />
      <Button fx:id="backButton" layoutX="625.0" layoutY="359.0" mnemonicParsing="false" onAction="#backButtonPressed" prefHeight="27.0" prefWidth="61.0" text="Back" />
      <Button fx:id="addUserButton" layoutX="93.0" layoutY="359.0" mnemonicParsing="false" onAction="#addButtonPressed" prefHeight="27.0" prefWidth="61.0" text="Add" />
   </children>
</AnchorPane>
